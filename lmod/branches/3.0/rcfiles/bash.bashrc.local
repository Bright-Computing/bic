if [ -d /usr/local/Cluster-Config/modulefiles ]; then
  if [ -f /etc/profile.d/modules.sh ]; then
    . /etc/profile.d/modules.sh
    module load dot
  fi
else
  echo "--> No modules environment"
fi
if [ "x$SHLVL" != "x1" ]; then
  export PS1="SUBSHELL> $PS1"
fi
