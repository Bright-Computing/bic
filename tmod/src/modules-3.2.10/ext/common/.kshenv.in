# start .kshenv
case "$0" in
          -sh|sh|*/sh)	modules_shell=sh ;;
       -ksh|ksh|*/ksh)	modules_shell=ksh ;;
    -bash|bash|*/bash)	modules_shell=bash ;;
esac
module() { eval `@BASEPREFIX@/Modules/$MODULE_VERSION/bin/modulecmd $modules_shell $*`; }

if [ -f ${ENV:-$HOME/.kshenv.ext} ]; then
	. ${ENV:-$HOME/.kshenv.ext}
fi

